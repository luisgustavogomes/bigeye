                                <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                                    <stops>
                                        <Stop color="BLACK" />
                                        <Stop color="#255ea4" offset="1.0" />
                                    </stops>
                                </RadialGradient>
                            </ripplerFill>
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
