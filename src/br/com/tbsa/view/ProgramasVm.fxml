                                <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                                    <stops>
                                        <Stop color="BLACK" />
                                        <Stop color="#255ea4" offset="1.0" />
                                    </stops>
                                </RadialGradient>
                            </ripplerFill>
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../img/folders%20(7).png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </JFXButton>
                    </children>
                    <HBox.margin>
                        <Insets left="8.0" right="8.0" top="25.0" />
                    </HBox.margin>
                </VBox>
                <HBox HBox.hgrow="ALWAYS">
                    <children>
                        <GridPane prefHeight="428.0" prefWidth="755.0" HBox.hgrow="ALWAYS">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <TableView id="id" fx:id="tvProgramasVm" onMouseClicked="#handlerEdicao" prefHeight="456.0" prefWidth="880.0" styleClass="tableColumn" stylesheets="@../../../../StyleCRUDedit.css" tableMenuButtonVisible="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                                    <columns>
                                        <TableColumn id="id" fx:id="idProgramasVmCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="ID" />
                                        <TableColumn fx:id="programaCol" prefWidth="742.0" text="PROGRAMA" /> 
                                        <TableColumn fx:id="vmCol" prefWidth="742.0" text="VM" />
                                        <TableColumn fx:id="dataInstalacaoCol" prefWidth="742.0" text="DATA INSTALAÇÃO" />
                                    </columns>
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                    </columnResizePolicy>
                                </TableView>
                            </children>
                        </GridPane>
                    </children>
                </HBox>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
