                        <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
                            <stops>
                                <Stop color="BLACK" />
                                <Stop color="#255ea4" offset="1.0" />
                            </stops>
                        </RadialGradient>
                    </ripplerFill>
                    <graphic>
                        <ImageView fitHeight="40.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../img/folders%20(4).png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
            </children>
        </HBox>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <VBox prefHeight="396.0" prefWidth="18.0" spacing="10.0">
                    <children>
                        <JFXButton id="bt" fx:id="btnServico" contentDisplay="GRAPHIC_ONLY" onAction="#hendlerServico" prefWidth="50.0" styleClass="btns" text="Serviços" VBox.vgrow="ALWAYS">
